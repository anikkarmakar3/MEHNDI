<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MehndiInterior" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="colorOnPrimary">@color/white</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!--        <item name="android:fontFamily">serif</item>-->
        <!--        <item name="fontFamily">serif</item>-->
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:windowDisablePreview">true</item>
    </style>

    <style name="myButton">
        <item name="android:textStyle">bold</item>
        <item name="android:elevation">15dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:foreground">@drawable/foreground_click_effect_crner20dp</item>
        <!--        <item name="android:backgroundTint">@color/thirty11</item>-->
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/shape_corner20dp</item>
        <item name="android:tint">@color/white</item>
    </style>

    <style name="myBackround">
        <item name="android:elevation">15dp</item>
        <item name="android:background">@color/sixty1</item>
        <item name="android:backgroundTint">@color/sixty1</item>
    </style>

    <style name="myBackround2">
        <item name="android:elevation">15dp</item>
        <item name="android:background">@color/thirty11</item>
        <item name="android:backgroundTint">@color/thirty11</item>
    </style>

    <style name="myText1">
        <item name="android:textColor">@color/thirty1</item>
    </style>

    <style name="myText2">
        <item name="android:textColor">#4097F4</item>
    </style>

    <style name="myText3">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="myText4">
        <item name="fontFamily">sans-serif-medium</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="myEdittext">
        <item name="android:elevation">15dp</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <!--        <item name="backgroundTint">@color/thirty1</item>-->
        <item name="android:paddingLeft">10dp</item>
        <item name="android:background">@drawable/shape_corner20dp</item>
    </style>

    <style name="myEditTextText">
        <item name="android:elevation">15dp</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:textColor">@color/black</item>
        <item name="backgroundTint">@color/thirty1</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:background">@drawable/shape_corner20dp</item>
    </style>

    <style name="myDashboardImage">
        <item name="android:tint">@color/ten1</item>
    </style>

    <style name="myDashboard">
        <item name="android:elevation">15dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:foreground">@drawable/foreground_click_effect_crner</item>
    </style>

    <style name="Theme.MehndiInterior.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MehndiInterior.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.MehndiInterior.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
